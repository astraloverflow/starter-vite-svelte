{
  "private": true,
  "license": "",
  "version": "2021.06.24",
  "name": "@astraloverflow/starter-vite-svelte",
  "description": "A starter template for web projects using vitejs and svelte",
  "scripts": {
    "test": "stylelint \"src/**/*.{css}\" && eslint \"src/**/*.{js,ts}\" && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write",
    "dev": "vite",
    "build": "svelte-check --tsconfig ./tsconfig.json && vite build",
    "serve": "vite preview"
  },
  "browserslist": [
    "last 2 major versions and cover 95% and not dead and not ie > 1 and not op_mini all"
  ],
  "dependencies": {
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.1",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  }
}
